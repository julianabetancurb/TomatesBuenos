@model List<Movie>

<h1>Top Movies</h1>

<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Image</th
            
        </tr>
    </thead>
    <tbody>
        @foreach (var movie in Model)
        {
                <tr>
                    <td>@movie.Title</td>
                    <td><img src="@movie.ImageURL" alt="Movie Image"></td>
                    <td>@movie.Genre</td>
                </tr>
        }
    </tbody>
</table>

